
@model IEnumerable<EFW_Assignment.Models.Course>
@using EFW_Assignment.Models

@{
    ViewBag.Title = "Course List";
}
<p>
    @Html.ActionLink("Add New Course", "CreateCourse")
</p>
<table class="table">
    <tr>

        <th>
            Id
        </th>
        <th>
            Course Name
        </th>
        <th>
            Time Period
        </th>
        @*<th>
                Teacher Id
            </th>*@
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        @*@Html.Partial("_PeopleList")*@
        <partial name="_CourseList" model="item" />
    }
</table>